class Solution:
    def hasIncreasingSubarrays(self, nums: List[int], k: int) -> bool:
        mx = 0
        pre = 0
        cur = 0
        n = len(nums)
        for i in range(n):
            cur += 1
            if i == n - 1 or nums[i] >= nums[i+1]:
                if cur >= 2*k:
                    return True
                if pre >= k and cur  >= k:
                    return True
                pre = cur
                cur = 0
        return False
    
